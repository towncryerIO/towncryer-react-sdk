{
  "name": "@towncryerio/towncryer-react-sdk",
  "version": "1.0.0-f34dbd5",
  "description": "React SDK for integrating with the Towncryer API, wrapping the core SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc && tsc -p tsconfig.esm.json",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint src --ext .tsx,.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "use-local-api": "npm install file:../typescript-core-sdk",
    "use-remote-api": "npm install @towncryerio/towncryer-js-sdk@latest"
  },
  "keywords": [
    "towncryer",
    "react",
    "sdk",
    "notifications",
    "api",
    "client"
  ],
  "author": "VolvLabs",
  "license": "Commercial",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/blocks": "^7.0.0",
    "@storybook/react": "^7.0.0",
    "@storybook/react-vite": "^7.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "rimraf": "^5.0.1",
    "storybook": "^7.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@towncryerio/towncryer-js-sdk": "^1.2.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
